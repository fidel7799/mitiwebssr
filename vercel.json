{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    { "src": "api/server.mjs", "use": "@vercel/node" },
    { "src": "dist/mitiwebssr/browser/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/mitiwebssr/browser/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/dist/mitiwebssr/browser/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server"
    }
  ]
}
