{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist/mitiwebssr/browser"
      }
    },
    {
      "src": "api/server.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/mitiwebssr/browser/**",
          "dist/mitiwebssr/server/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/server"
    }
  ]
}
